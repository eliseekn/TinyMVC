<?php

namespace App\Mails;

use Core\Routing\View;
use Core\Support\Mailer;

class CLASSNAME
{
    public static function send(string $email)
    {
        return Mailer::to($email)
            ->from(config('mailer.sender.email'), config('mailer.sender.name'))
            ->reply(config('mailer.sender.email'), config('mailer.sender.name'))
			->subject('Subject')
            ->html(View::getContent('emails.RESOURCENAME'))
			->send();
    }
}
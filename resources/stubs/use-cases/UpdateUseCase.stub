namespace NAMESPACE\MODELNAME;

use App\Database\Models\MODELNAME;

final class UpdateUseCase
{
    public function handle(array $data, int $id): bool
	{
        $MODELNAME = MODELNAME::find($id);
        $MODELNAME->fill($data);

        return $MODELNAME->save();
	}
}

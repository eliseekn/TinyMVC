{
    "name": "eliseekn/tinymvc",
    "description": "PHP framework based on MVC architecture",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "N'Guessan Kouadio Elis√©e",
            "email": "eliseekn@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "app/Functions.php",
            "core/Support/Helpers.php"
        ],
        "psr-4": {
            "App\\" : "app/",
            "Core\\" : "core/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "require": {
        "php": "^7.2",
        "wixel/gump": "^1.11",
        "phpmailer/phpmailer": "^6.1",
        "nesbot/carbon": "^2.41",
        "twig/twig": "^3.0",
        "symfony/console": "^5.2",
        "dflydev/dot-access-data": "^3.0",
        "spatie/async": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/var-dumper": "^5.2",
        "filp/whoops": "^2.12"
    }
}
